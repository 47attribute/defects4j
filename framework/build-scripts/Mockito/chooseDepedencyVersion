#!/bin/bash
# $1 = builddir
# $2 = scriptdir

rm $2/build-scripts/Mockito/lib/byte-buddy*
cd $1
./gradlew dependencies >> tmpDepend.txt
version=`cat tmpDepend.txt | grep buddy | cut -d: -f 3 | head -1`
cp $2/build-scripts/Mockito/mVersionLib/byte-buddy-$version.jar $2/build-scripts/Mockito/lib
rm tmpDepend.txt
